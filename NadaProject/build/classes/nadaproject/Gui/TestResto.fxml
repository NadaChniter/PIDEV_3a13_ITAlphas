<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="NadaProject.Gui.TestRestoController">
   <children>
      <ImageView fitHeight="407.0" fitWidth="600.0" opacity="0.72">
         <image>
            <Image url="@BckResto.jpg" />
         </image>
      </ImageView>
      <VBox layoutX="14.0" layoutY="103.0" opacity="0.89" prefHeight="242.0" prefWidth="278.0" style="-fx-background-color: #ffffff;">
         <children>
            <Button fx:id="MenuBtn" mnemonicParsing="false" prefHeight="40.0" prefWidth="258.0" style="-fx-background-color: #F21111;" text="Check Today's menu" textFill="WHITE">
               <VBox.margin>
                  <Insets bottom="20.0" left="25.0" right="20.0" top="60.0" />
               </VBox.margin>
               <font>
                  <Font name="System Bold" size="19.0" />
               </font>
               <padding>
                  <Insets right="30.0" />
               </padding>
            </Button>
            <Button fx:id="InfoBtn" mnemonicParsing="false" prefHeight="40.0" prefWidth="230.0" style="-fx-background-color: #0017A7;" text="Consult Informations" textAlignment="CENTER" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="19.0" />
               </font>
               <padding>
                  <Insets right="30.0" />
               </padding>
               <VBox.margin>
                  <Insets bottom="20.0" left="30.0" right="20.0" top="10.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
      <Button fx:id="Backbtn" layoutX="528.0" layoutY="358.0" mnemonicParsing="false" style="-fx-background-color: #0017A7;" text="BACK" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
